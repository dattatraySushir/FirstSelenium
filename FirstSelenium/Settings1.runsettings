<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<MSTest>
		<!--- Here we are doing the parallel execution-->
		<Parallelize>
			<Workers>2</Workers>
			<Scope>MethodLevel</Scope>
		</Parallelize>
	</MSTest>
	<TestRunParameters>

		<Parameter name="Url" value="http://rmgyantra:rmgy@9999@localhost:8084/" />
		<Parameter name="Browser" value="Chrome" />

		<Parameter name="username" value="rmgyantra" />
		<Parameter name="password" value="rmgy@9999" />

		<Parameter name="myname" value="rama" />

	</TestRunParameters>

	<LoggerRunSettings>
		<Loggers>
			<Logger friendlyName="console" enabled="True">
				<Configuration>
					<Verbosity>quiet</Verbosity>
				</Configuration>
			</Logger>
			<Logger friendlyName="trx" enabled="True">
				<Configuration>
					<LogFileName>Browser.trx</LogFileName>
				</Configuration>
			</Logger>
			<Logger friendlyName="html" enabled="True">
				<Configuration>
					<LogFileName>ItIsaHtmlReport.html</LogFileName>
				</Configuration>
			</Logger>
			<Logger friendlyName="blame" enabled="True" />
		</Loggers>
	</LoggerRunSettings>


</RunSettings>